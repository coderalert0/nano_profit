{
  "name": "margindash",
  "version": "0.1.0",
  "description": "MarginDash SDK for tracking AI usage and revenue",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ai",
    "llm",
    "margin",
    "tracking",
    "margindash"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  }
}
